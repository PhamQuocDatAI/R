VariableInfo <-function(X1, Choice1){
if (Choice1== "Zeros") {
D1= (X1==0)
}
if (Choice1== "NAs") {
D1 <- is.na(X1)
}
colSums(D1, na.rm=TRUE)
}
VariableInfo(CP,"Zeros")
VariableInfo(CP,"NAs")
VariableInfo(CP,"thamso")
VariableInfo(CP)
