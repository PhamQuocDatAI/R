VariableInfo <-function(X1, Choice1="Zeros"){
if (Choice1== "Zeros") {
D1= (X1==0)
}
if (Choice1== "NAs") {
D1 <- is.na(X1)
}
if (Choice1!= "Zeros" & Choice1 != "NAs") {
print("You make a miss typo.You should choose: Zeros or NAs")
}else{
colSums(D1, na.rm=TRUE)
}
}
VariableInfo(CP,"Zeros")
VariableInfo(CP,"NAs")
VariableInfo(CP,"thamso")
